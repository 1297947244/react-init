{
  "name": "shequ",
  "version": "0.0.1",
  "private": true,
  "description": "A frontend boilerplate with React hooks, Redux and Typescript",
  "keywords": [
    "react",
    "hooks",
    "redux",
    "typescript",
    "router5",
    "antd4.x"
  ],
  "main": "index.js",
  "scripts": {
    "analyz": "NODE_ENV=production npm_config_report=true npm run build",
